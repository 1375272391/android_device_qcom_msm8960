# Master file for snd_soc_msm_auto sound card
# Lists all use case verbs
SectionUseCase."HiFi" {
Comment "Music playback use case"
SectionVerb
	Name "HiFi"

	EnableSequence
		'MI2S_RX Audio Mixer MultiMedia1':1:1
	EndSequence

	DisableSequence
		'MI2S_RX Audio Mixer MultiMedia1':1:0
	EndSequence

	# ALSA PCMs
	CapturePCM 0
	PlaybackPCM 0

EndSection

SectionVerb
	Name "HiFiBT SCO Rx"

	EnableSequence
		'AUX_PCM_RX Audio Mixer MultiMedia1':1:1
	EndSequence

	DisableSequence
		'AUX_PCM_RX Audio Mixer MultiMedia1':1:0
	EndSequence

	# ALSA PCMs
	CapturePCM 0
	PlaybackPCM 0

EndSection

